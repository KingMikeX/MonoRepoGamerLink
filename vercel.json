{
  "$schema": "https://vercel.com/schemas/project/config.json",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "apps/webapp/.next",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/Backend/src/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "apps/webapp/$1"
    }
  ]
}